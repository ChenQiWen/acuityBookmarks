{
  "name": "acuity-bookmarks-backend",
  "version": "1.0.0",
  "description": "Backend for AcuityBookmarks - Cloudflare Worker",
  "type": "module",
  "main": "cloudflare-worker.js",
  "scripts": {
    "dev": "wrangler dev --local --port 8787 --local-protocol https --https-cert-path ./localhost+2.pem --https-key-path ./localhost+2-key.pem",
    "dev:remote": "wrangler dev --remote --port 8787 --local-protocol https --https-cert-path ./localhost+2.pem --https-key-path ./localhost+2-key.pem",
    "deploy": "wrangler deploy",
    "deploy:preview": "wrangler deploy --env preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "bun test",
    "audit": "bun audit",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:new": "supabase migration new",
    "logs": "wrangler tail",
    "health-check": "curl -f -k https://localhost:8787/api/health || exit 1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.79.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "supabase": "^1.187.3",
    "wrangler": "^3.63.1"
  },
  "runtime": "bun",
  "keywords": [
    "cloudflare-worker",
    "bookmark",
    "chrome-extension",
    "high-performance"
  ]
}
