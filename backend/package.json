{
  "name": "acuity-bookmarks-backend",
  "version": "1.0.0",
  "description": "Backend for AcuityBookmarks Chrome Extension - Pure Bun Native + Intelligent Web Crawler",
  "type": "module",
  "main": "server-bun-native.js",
  "scripts": {
    "start": "bun server-bun-native.js",
    "start:prod": "NODE_ENV=production bun server-bun-native.js",
    "dev": "bun --hot server-bun-native.js",
    "dev:enhanced": "bun scripts/dev-server.js",
    "dev:watch": "bun --watch server-bun-native.js",
    "dev:inspect": "bun --hot --inspect server-bun-native.js",
    "dev:verbose": "bun --hot --verbose server-bun-native.js",
    "build": "echo \"Bun native build completed - no transpilation needed\"",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:check": "eslint . --cache || echo '⚠️  ESLint found issues but continuing...'",
    "audit": "bun audit",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "dev:setup": "../scripts/dev-setup.sh",
    "benchmark": "bun benchmark.js",
    "performance": "bun --prof server-bun-native.js"
  },
  "dependencies": {
    "uuid": "^11.1.0",
    "cheerio": "^1.0.0-rc.12",
    "zod": "^3.22.4"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bun": "latest",
    "eslint": "^9.35.0"
  },
  "runtime": "bun",
  "keywords": [
    "bun",
    "native",
    "bookmark",
    "chrome-extension",
    "high-performance"
  ]
}
