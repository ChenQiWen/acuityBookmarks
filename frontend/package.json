{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && node scripts/clean-dist.cjs",
    "build:watch": "vite build --watch",
    "build:hot": "node scripts/watch-build.js",
    "build:dev": "concurrently \"npm run build:watch\" \"npm run watch:clean\"",
    "watch:clean": "nodemon --watch ../dist --ext html,js,css --exec \"node scripts/clean-dist.cjs\"",
    "clean-dist": "node scripts/clean-dist.cjs",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.11.0",
    "lodash-es": "^4.17.21",
    "sortablejs-vue3": "^1.3.0",
    "vue": "^3.5.18",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.0",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.7",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.8",
    "vue-tsc": "^3.0.5"
  }
}
