{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && node scripts/clean-dist.cjs",
    "build:prod": "NODE_ENV=production vue-tsc -b && vite build && node scripts/clean-dist.cjs",
    "build:watch": "vite build --watch",
    "build:hot": "node scripts/watch-build.js",
    "build:dev": "concurrently \"npm run build:watch\" \"npm run watch:clean\"",
    "build:analyze": "ANALYZE=true vite build",
    "watch:clean": "nodemon --watch ../dist --ext html,js,css --exec \"node scripts/clean-dist.cjs\"",
    "clean-dist": "node scripts/clean-dist.cjs",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.ts",
    "test:integration": "vitest run --reporter=verbose src/**/*.integration.test.ts",
    "test:e2e": "echo \"E2E tests not implemented yet\"",
    "type-check": "vue-tsc --noEmit",
    "lint": "echo \"Linting not configured yet\"",
    "dev:setup": "../scripts/dev-setup.sh",
    "dev:start": "../scripts/dev-start.sh",
    "performance:monitor": "echo \"Performance monitoring enabled in development builds\""
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tanstack/vue-virtual": "^3.13.12",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "sortablejs-vue3": "^1.3.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.0",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.7",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^2.1.8",
    "vue-tsc": "^3.0.5"
  }
}
