{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && node scripts/clean-dist.cjs",
    "build:prod": "NODE_ENV=production vue-tsc -b && vite build && node scripts/clean-dist.cjs",
    "build:watch": "vite build --watch",
    "build:hot": "node scripts/watch-build.js",
    "build:hot:no-lint": "SKIP_ESLINT=true node scripts/watch-build.js",
    "build:dev": "concurrently \"npm run build:watch\" \"npm run watch:clean\"",
    "build:analyze": "ANALYZE=true vite build",
    "watch:clean": "nodemon --watch ../dist --ext html,js,css --exec \"node scripts/clean-dist.cjs\"",
    "clean-dist": "node scripts/clean-dist.cjs",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:check": "eslint . --cache --max-warnings 0",
    "dev:setup": "../scripts/dev-setup.sh",
    "dev:start": "../scripts/dev-start.sh",
    "performance:monitor": "echo \"Performance monitoring enabled in development builds\""
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tanstack/vue-virtual": "^3.13.12",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "sortablejs-vue3": "^1.3.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/chrome": "^0.1.4",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "nodemon": "^3.1.7",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
