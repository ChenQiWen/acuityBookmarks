{
  "mcpServers": {
    "filesystem": {
      "command": "uvx",
      "args": [
        "mcp-server-filesystem",
        "/Users"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "github": {
      "command": "uvx",
      "args": [
        "mcp-server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "你的GitHub Token",
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "brave-search": {
      "command": "uvx",
      "args": [
        "mcp-server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "你的Brave API Key",
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browser-url=http://127.0.0.1:9222"
      ],
      "disabled": false,
      "autoApprove": [
        "new_page",
        "take_snapshot",
        "fill",
        "click",
        "evaluate_script",
        "wait_for",
        "navigate_page",
        "take_screenshot",
        "press_key"
      ]
    }
  }
}